#!/bin/bash
#SBATCH -t 48:00:00
#SBATCH --exclusive
#SBATCH -p par7.q
#SBATCH -N 1
#SBATCH --ntasks-per-node=1

source /etc/profile.d/modules.sh

module load intel/xe_2018.2
module load intelmpi/intel/2018.2


export d=2
export cores=3
for flops in 1 100 10000 1000000
done
 for h in 0.1 0.5 0.01 0.005 0.001
 do
   mpirun ./PeanoRegularGridUpscaling$dd-parallel $h $f $cores> shared-memory-$cores-cores-$dd-$h-$flops-flops.out 6
 done
done

